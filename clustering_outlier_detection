   "source": [
    "print(\"Generating cluster labels based on attention-weighted tokens...\\n\")\n",
    "\n",
    "cluster_labels_dict = {}\n",
    "all_texts = df_filtered['text_contents_punctuated'].tolist()\n",
    "\n",
    "for cluster_id in range(NUM_CLUSTERS):\n",
    "    cluster_indices = df_filtered[df_filtered['cluster'] == cluster_id].index.tolist()\n",
    "    cluster_texts = [all_texts[i] for i in cluster_indices]\n",
    "    cluster_embeddings_data = [embeddings_data[i] for i in cluster_indices]\n",
    "    \n",
    "    print(f\"\\n{'='*80}\")\n",
    "    print(f\"Cluster {cluster_id} ({len(cluster_texts)} documents)\")\n",
    "    print(f\"{'='*80}\")\n",
    "    \n",
    "    # Extract top attention-weighted tokens\n",
    "    top_tokens = extract_top_attention_tokens(cluster_texts, cluster_embeddings_data, top_n=15)\n",
    "    print(f\"\\nTop attention-weighted tokens:\")\n",
    "    for token, score in top_tokens[:10]:\n",
    "        print(f\"  {token} (权重: {score:.2f})\")\n",
    "    \n",
    "    # Extract distinctive phrases\n",
    "    distinctive_phrases = extract_distinctive_phrases(cluster_texts, all_texts, top_n=10)\n",
    "    print(f\"\\nDistinctive phrases:\")\n",
    "    if distinctive_phrases:\n",
    "        for phrase, freq, distinctiveness in distinctive_phrases[:8]:\n",
    "            print(f\"  {phrase} (出现{freq}次, 特异性{distinctiveness:.2f})\")\n",
    "    else:\n",
    "        print(\"  (No distinctive phrases found)\")\n",
    "    \n",
    "    # Show sample text\n",
    "    print(f\"\\nSample text:\")\n",
    "    sample_text = cluster_texts[0]\n",
    "    preview = sample_text[:150] + '...' if len(sample_text) > 150 else sample_text\n",
    "    print(f\"  {preview}\")\n",
    "    \n",
    "    # Generate four-character content summary label\n",
    "    suggested_label = generate_four_char_summary(top_tokens, distinctive_phrases)\n",
    "    \n",
    "    print(f\"\\n💡 Four-character content summary: {suggested_label}\")\n",
    "    print(f\"   (Generated from attention-weighted tokens and distinctive phrases)\")\n",
    "    \n",
    "    cluster_labels_dict[cluster_id] = suggested_label\n",
    "\n",
    "print(\"\\n\" + \"=\"*80)\n",
