   "source": [
    "print(\"Performing PCA for 2D visualization...\")\n",
    "pca = PCA(n_components=2, random_state=42)\n",
    "embeddings_2d = pca.fit_transform(embeddings_matrix)\n",
    "\n",
    "df_filtered['x'] = embeddings_2d[:, 0]\n",
    "df_filtered['y'] = embeddings_2d[:, 1]\n",
    "\n",
    "# Create visualization\n",
    "fig, axes = plt.subplots(1, 2, figsize=(18, 8))\n",
    "\n",
    "# Plot 1: Clusters\n",
    "ax1 = axes[0]\n",
    "colors = plt.cm.Set3(range(NUM_CLUSTERS))\n",
    "\n",
    "for cluster_id in range(NUM_CLUSTERS):\n",
    "    cluster_data = df_filtered[df_filtered['cluster'] == cluster_id]\n",
    "    label = cluster_labels_dict[cluster_id]\n",
    "    \n",
    "    ax1.scatter(\n",
    "        cluster_data['x'], \n",
    "        cluster_data['y'],\n",
    "        c=[colors[cluster_id]],\n",
    "        label=f\"{label} (n={len(cluster_data)})\",\n",
    "        s=100,\n",
    "        alpha=0.7,\n",
    "        edgecolors='black',\n",
    "        linewidth=0.5\n",
    "    )\n",
    "\n",
    "ax1.set_title(f'{DOCUMENT_TYPE} 语义聚类可视化\\n(SIKU-BERT + K-Means)', fontsize=16, pad=20)\n",
    "ax1.set_xlabel('主成分 1', fontsize=12)\n",
    "ax1.set_ylabel('主成分 2', fontsize=12)\n",
    "ax1.legend(loc='best', fontsize=9, framealpha=0.9)\n",
    "ax1.grid(True, alpha=0.3)\n",
    "\n",
    "# Plot 2: Outliers highlighted\n",
    "ax2 = axes[1]\n",
    "\n",
    "# Plot normal points\n",
    "normal_data = df_filtered[~df_filtered['is_outlier']]\n",
    "outlier_data = df_filtered[df_filtered['is_outlier']]\n",
    "\n",
    "ax2.scatter(\n",
    "    normal_data['x'],\n",
    "    normal_data['y'],\n",
    "    c='lightgray',\n",
    "    s=80,\n",
    "    alpha=0.5,\n",
    "    label=f'正常文本 (n={len(normal_data)})'\n",
    ")\n",
    "\n",
    "# Plot outliers\n",
    "ax2.scatter(\n",
    "    outlier_data['x'],\n",
    "    outlier_data['y'],\n",
    "    c='red',\n",
    "    s=150,\n",
    "    alpha=0.8,\n",
    "    edgecolors='darkred',\n",
    "    linewidth=2,\n",
    "    marker='X',\n",
    "    label=f'异常文本 (n={len(outlier_data)})'\n",
    ")\n",
    "\n",
    "ax2.set_title(f'{DOCUMENT_TYPE} 异常检测可视化\\n(LOF + 距离法)', fontsize=16, pad=20)\n",
    "ax2.set_xlabel('主成分 1', fontsize=12)\n",
    "ax2.set_ylabel('主成分 2', fontsize=12)\n",
    "ax2.legend(loc='best', fontsize=10, framealpha=0.9)\n",
    "ax2.grid(True, alpha=0.3)\n",
    "\n",
    "plt.tight_layout()\n",
    "output_file = f'clustering_outliers_{DOCUMENT_TYPE}.png'\n",
    "plt.savefig(output_file, dpi=300, bbox_inches='tight')\n",
    "print(f\"\\n✅ Visualization saved to: {output_file}\")\n",
    "plt.show()"
   ]
