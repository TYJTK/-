    "    'lof_score',\n",
    "    'is_outlier',\n",
    "    'is_outlier_distance',\n",
    "    'is_outlier_lof',\n",
    "    'text_contents_punctuated'\n",
    "]\n",
    "\n",
    "# Filter to only include columns that exist\n",
    "output_columns = [col for col in output_columns if col in df_filtered.columns]\n",
    "df_output = df_filtered[output_columns].copy()\n",
    "\n",
    "# Save full results\n",
    "output_file = f'clustering_outliers_results_{DOCUMENT_TYPE}.csv'\n",
    "df_output.to_csv(output_file, index=False, encoding='utf-8-sig')\n",
    "print(f\"✅ Full results saved to: {output_file}\")\n",
    "\n",
    "# Save outliers only\n",
    "outliers_file = f'outliers_only_{DOCUMENT_TYPE}.csv'\n",
    "df_outliers = df_output[df_output['is_outlier']].copy()\n",
    "df_outliers = df_outliers.sort_values('lof_score', ascending=False)\n",
    "df_outliers.to_csv(outliers_file, index=False, encoding='utf-8-sig')\n",
    "print(f\"✅ Outliers saved to: {outliers_file}\")\n",
    "\n",
    "print(f\"\\n{'='*80}\")\n",
    "print(\"Summary Statistics:\")\n",
    "print(f\"  Total texts analyzed: {len(df_filtered)}\")\n",
    "print(f\"  Number of clusters: {NUM_CLUSTERS}\")\n",
    "print(f\"  Outliers detected: {len(df_outliers)} ({len(df_outliers)/len(df_filtered)*100:.1f}%)\")\n",
    "print(f\"  Average silhouette score: {silhouette:.3f}\")\n",
    "print(f\"{'='*80}\")"
   ]
