   "source": [
    "# Count outliers per cluster\n",
    "outlier_stats = df_filtered.groupby('cluster').agg({\n",
    "    'is_outlier': ['sum', 'count']\n",
    "}).reset_index()\n",
    "\n",
    "outlier_stats.columns = ['cluster', 'num_outliers', 'total_texts']\n",
    "outlier_stats['outlier_percentage'] = (outlier_stats['num_outliers'] / outlier_stats['total_texts'] * 100).round(1)\n",
    "outlier_stats['cluster_label'] = outlier_stats['cluster'].map(cluster_labels_dict)\n",
    "\n",
    "# Reorder columns\n",
    "outlier_stats = outlier_stats[['cluster', 'cluster_label', 'total_texts', 'num_outliers', 'outlier_percentage']]\n",
    "outlier_stats = outlier_stats.sort_values('outlier_percentage', ascending=False)\n",
    "\n",
    "print(\"Outlier Distribution by Cluster:\")\n",
    "print(\"=\"*80)\n",
    "print(outlier_stats.to_string(index=False))\n",
    "\n",
    "# Visualize\n",
    "fig, ax = plt.subplots(figsize=(12, 6))\n",
    "\n",
    "bars = ax.bar(\n",
    "    range(len(outlier_stats)),\n",
    "    outlier_stats['outlier_percentage'],\n",
    "    color=colors[:len(outlier_stats)],\n",
    "    edgecolor='black',\n",
    "    linewidth=1.5,\n",
    "    alpha=0.8\n",
    ")\n",
    "\n",
    "ax.set_xticks(range(len(outlier_stats)))\n",
    "ax.set_xticklabels(outlier_stats['cluster_label'], rotation=0, fontsize=11)\n",
    "ax.set_xlabel('聚类标签', fontsize=12)\n",
    "ax.set_ylabel('异常文本比例 (%)', fontsize=12)\n",
    "ax.set_title(f'{DOCUMENT_TYPE} 各聚类异常文本分布', fontsize=14, pad=15)\n",
    "ax.grid(axis='y', alpha=0.3)\n",
    "\n",
    "# Add percentage labels on bars\n",
    "for i, bar in enumerate(bars):\n",
    "    height = bar.get_height()\n",
    "    ax.text(bar.get_x() + bar.get_width()/2., height,\n",
    "            f'{height:.1f}%',\n",
    "            ha='center', va='bottom', fontsize=10, fontweight='bold')\n",
    "\n",
    "plt.tight_layout()\n",
    "output_file = f'outlier_distribution_{DOCUMENT_TYPE}.png'\n",
    "plt.savefig(output_file, dpi=300, bbox_inches='tight')\n",
    "print(f\"\\n✅ Outlier distribution chart saved to: {output_file}\")\n",
    "plt.show()"
   ]
