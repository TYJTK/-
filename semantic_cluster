    "# Model configuration - using local copy\n",
    "model_path = \"./sikubert\"\n",
    "\n",
    "print(f\"Loading SIKU-BERT tokenizer and model from local directory: {model_path}\")\n",
    "tokenizer = AutoTokenizer.from_pretrained(\n",
    "    model_path,\n",
    "    trust_remote_code=True\n",
    ")\n",
    "\n",
    "print(\"Loading SIKU-BERT model...\")\n",
    "\n",
    "# Configure device for optimal performance\n",
    "if torch.cuda.is_available():\n",
    "    device = torch.device('cuda')\n",
    "    print(f\"ğŸš€ GPU detected: {torch.cuda.get_device_name(0)}\")\n",
    "    print(f\"   GPU Memory: {torch.cuda.get_device_properties(0).total_memory / 1024**3:.1f} GB\")\n",
    "else:\n",
    "    device = torch.device('cpu')\n",
    "    print(\"âš ï¸  GPU not available, using CPU (this will be slower)\")\n",
    "\n",
    "model = AutoModel.from_pretrained(\n",
    "    model_path,\n",
    "    dtype=torch.float16 if torch.cuda.is_available() else torch.float32,\n",
    "    trust_remote_code=True\n",
    ")\n",
    "\n",
    "# Move model to GPU if available\n",
    "model = model.to(device)\n",
    "model.eval()\n",
    "\n",
    "print(f\"âœ… SIKU-BERT model loaded successfully on {device}!\")"
