    "    f.write(\"=\"*100 + \"\\n\\n\")\n",
    "\n",
    "    for idx, row in df_cluster_analysis.head(20).iterrows():\n",
    "        f.write(f\"\\nCluster {row['cluster_id']} - Rank #{idx+1}\\n\")\n",
    "        f.write(f\"{'-'*100}\\n\")\n",
    "        f.write(f\"Frequency: {row['n_passages']} passages across {row['n_edicts']} edicts\\n\")\n",
